{
    "editor.wordWrap": "on",
    "editor.fontSize": 16,
    "window.zoomLevel": -1,
    "workbench.colorTheme": "Big Bang -> Pitch Black Theme",
    "terminal.integrated.fontSize": 15,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
      "activityBar.foreground": "#ffc400",
      "activityBarBadge.background": "#d0071b",
      "activityBarBadge.foreground": "#0f0f0f",
      // Border separating the title bar from the rest of the editor
      "titleBar.border": "#ff8400",
      // Border color to separate Tabs from each other
      "tab.border": "#ff8400",
      // Border color on the top separating to the editor (terminal)
      "panel.border": "#ff8400",
      // Border color on the side separating the editor
      "sideBar.border": "#ff8400",
      // Border color with the Side Bar
      "activityBar.border": "#ff8400",
      // Border color separating the Status Bar and editor
      "statusBar.border": "#ff8400",
      // Border color to separate multiple editor groups from each other
      "editorGroup.border": "#ff8400",
      // Border color of the editor group title header when tabs are enabled
      "editorGroupHeader.tabsBorder": "#ff8400"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#ffea00",
        "keywords": "#5ff001",
        "strings": "#5ff001",
        "types": "#00ff22",
        "functions": "#5ff001",
        "numbers": "#00ff1a",
        "variables": "#fff700"
    }
}